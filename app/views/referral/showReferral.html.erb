  <script>
    var code = window.location.hash;
    code = code.replace('#','');
    $('#refcode').html(code);
    $('#message').attr('href', 'sms: ?body=Hi, Try Shuttl and travel to your office at just Rs 49. Use my code '+code+' to signup and get 2 FREE RIDES. Click here http://bit.ly/2delqW0 to download the Shuttl APP.','_system');
    $('#whatsapp').attr('href', 'whatsapp://send?text=Hi, Try Shuttl and travel to your office at just Rs 49. Use my code '+code+' to signup and get 2 FREE RIDES. Click here http://bit.ly/2cpZO7u to download the Shuttl APP.');
    $('#email').attr('href', 'mailto:?&subject=travel to your office at just Rs 49&body=Hi, Try Shuttl and travel to your office at just Rs 49. Use my code '+code+' to signup and get 2 FREE RIDES. Click here http://bit.ly/2delqW0 to download the Shuttl APP.');

    $('#whatsapp').on('click', function(e){
      var cb = generate_callback($(this));
      event.preventDefault();
      mixpanel.track("Custom_Referral", {
        "Referral_Code": code ,
        "Option": "Whatsapp" ,
      }, cb);
      setTimeout(cb, 500);
    });
    $('#message').on('click', function(e){
      var cb = generate_callback($(this));
      event.preventDefault();
      mixpanel.track("Custom_Referral", {
        "Referral_Code": code ,
        "Option": "Message" ,
      }, cb);
      setTimeout(cb, 500);
    });
    $('#email').on('click', function(e){
      var cb = generate_callback($(this));
      event.preventDefault();
      mixpanel.track("Custom_Referral", {
        "Referral_Code": code ,
        "Option": "Email" ,
      }, cb);
      setTimeout(cb, 500);
    });

    function generate_callback(a) {
      return function() {
        window.location = a.attr("href");
      }
    }
  </script>


</div></body></html>